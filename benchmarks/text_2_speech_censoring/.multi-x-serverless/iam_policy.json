{
    "aws": {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Action": [
                    "logs:CreateLogGroup",
                    "logs:CreateLogStream",
                    "logs:PutLogEvents"
                ],
                "Resource": "arn:aws:logs:*:*:*",
                "Effect": "Allow"
            },
            {
                "Action": [
                    "sns:Publish"
                ],
                "Resource": "arn:aws:sns:::*",
                "Effect": "Allow"
            }
        ]
    }
}